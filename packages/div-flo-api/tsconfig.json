{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist",
  "rootDir": ".",
    "baseUrl": ".",
    "paths": {
      "@div-flo/models": ["../../packages/div-flo-models/src"],
      "@div-flo/core": ["../../packages/div-flo-core/src"],
      "@div-flo/core/logging/*": ["../../packages/div-flo-core/src/logging/*"]
    },
    "composite": true
  },
    "include": [
      "src/**/*.ts",
      "src/**/*.js",
      "tests/**/*.ts",
      "tests/**/*.js",
      "../div-flo-core/src/logging/**/*.ts"
    ],
  "exclude": ["node_modules", "dist"],
  "references": [
    { "path": "../div-flo-models" },
    { "path": "../div-flo-core" }
  ]
}